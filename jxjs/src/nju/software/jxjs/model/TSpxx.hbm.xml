<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="nju.software.jxjs.model.TSpxx" table="T_SPXX" schema="dbo" catalog="JUDGE">
        <cache usage="read-write" />
        <id name="spxxbh" type="int">
            <column name="SPXXBH" />
            <generator class="assigned" />
        </id>
        <property name="spyj" type="string">
            <column name="SPYJ" not-null="true" />
        </property>
        
        <many-to-one name="spr" 
                     column="SPRBH"
                     class="nju.software.jxjs.model.PubXtglYhb"
                     cascade="none"
                     lazy="false"
                     fetch="join"/>
                     
        <many-to-one name="aj" 
                     column="AJBH"
                     class="nju.software.jxjs.model.PubAjJb"
                     cascade="none"
                     lazy="false"
                     fetch="join"/>            
                                    
    </class>
</hibernate-mapping>
