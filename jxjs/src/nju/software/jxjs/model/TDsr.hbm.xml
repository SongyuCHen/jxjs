<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="nju.software.jxjs.model.TDsr" table="T_DSR" schema="dbo" catalog="JUDGE">
    	<cache usage="read-write" />
        <id name="dsrbh" type="int">
            <column name="DSRBH" />
            <generator class="assigned" />
        </id>
        <property name="dsrxm" type="string">
        	<column name="DSRXM" not-null="true" />
        </property>
        
        <many-to-one name="dw" 
             column="DWBH"
             class="nju.software.jxjs.model.TDw"
             cascade="none"
             lazy="false"
             fetch="join"/>
        
        
    </class>
</hibernate-mapping>
